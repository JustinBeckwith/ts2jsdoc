{
  "name": "ts-jsdoc",
  "description": "Transform TypeScript to JSDoc annotated JS code",
  "version": "1.2.2",
  "license": "MIT",
  "bin": {
    "ts2jsdoc": "out/ts2jsdoc.js"
  },
  "main": "out/ts2jsdoc.js",
  "files": [
    "out",
    "*.js"
  ],
  "scripts": {
    "compile": "ts-babel .",
    "release": "ts-babel . && npm publish",
    "test": "yarn run compile && jest"
  },
  "dependencies": {
    "bluebird-lst": "^1.0.2",
    "chalk": "^1.1.3",
    "doctrine": "^2.0.0",
    "fs-extra-p": "^4.3.0",
    "source-map-support": "^0.4.15"
  },
  "devDependencies": {
    "@types/doctrine": "^0.0.3",
    "@types/node": "^7.0.18",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-transform-async-to-module-method": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-inline-imports-commonjs": "^1.2.0",
    "jest": "^20.0.1",
    "jest-environment-node-debug": "^2.0.0",
    "ts-babel": "^3.0.1"
  },
  "peerDependencies": {
    "typescript": "^2.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/develar/ts2jsdoc.git"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "keywords": [
    "typescript",
    "jsdoc"
  ],
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "test"
    ],
    "transformIgnorePatterns": [
      "<rootDir>"
    ],
    "testPathIgnorePatterns": [
      "[\\/]{1}fixtures[\\/]{1}"
    ],
    "testRegex": "\\.js$"
  }
}
