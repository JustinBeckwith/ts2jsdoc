{
  "name": "ts-jsdoc",
  "description": "Transform TypeScript to JSDoc annotated JS code",
  "version": "2.0.4",
  "license": "MIT",
  "bin": {
    "ts2jsdoc": "out/ts2jsdoc.js"
  },
  "main": "out/ts2jsdoc.js",
  "files": [
    "out",
    "*.js"
  ],
  "scripts": {
    "compile": "ts-babel .",
    "release": "ts-babel . && npm publish",
    "test": "yarn run compile && jest"
  },
  "dependencies": {
    "bluebird-lst": "^1.0.2",
    "chalk": "^2.0.1",
    "doctrine": "^2.0.0",
    "fs-extra-p": "^4.4.0",
    "source-map-support": "^0.4.15"
  },
  "devDependencies": {
    "@types/doctrine": "^0.0.3",
    "@types/node": "^8.0.19",
    "babel-preset-ts-node4-bluebird": "^0.1.0",
    "jest": "^20.0.4",
    "jest-environment-node-debug": "^2.0.0",
    "ts-babel": "^3.1.4"
  },
  "peerDependencies": {
    "typescript": "^2.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/develar/ts2jsdoc.git"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "keywords": [
    "typescript",
    "jsdoc"
  ],
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "test"
    ],
    "transformIgnorePatterns": [
      "<rootDir>"
    ],
    "testPathIgnorePatterns": [
      "[\\/]{1}fixtures[\\/]{1}"
    ],
    "testRegex": "\\.js$"
  },
  "babel": {
    "presets": [
      "babel-preset-ts-node4-bluebird"
    ]
  }
}
